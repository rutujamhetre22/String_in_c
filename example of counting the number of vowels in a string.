// example of counting the number of vowels in a string.



#include<stdio.h>  
#include<string.h>  
int main(){  
  char s[6]="rutuja";
   int i=0;
   int count=0;
   while(i<11){
       if(s[i]=='a'||s[i]=='i'||s[i]=='u'||s[i]=='o'||s[i]=='e'){
       count++;
   }
   i++;
   }
   printf("the numbers of vowels is %d",count);
   
    return 0;
}
 
