//Example of Null Characters

#include<stdio.h>  
#include<string.h>  
int main(){  
  char s[6]="rutuja";
   int i=0;
   int count=0;
   while(s[i]!=NULL){
       if(s[i]=='a'||s[i]=='i'||s[i]=='u'||s[i]=='o'||s[i]=='e'){
       count++;
   }
   i++;
   }
   printf("the numbers of vowels is %d",count);
   return 0;
}
 
 *******Output*********
 the numbers of vowels is 3
